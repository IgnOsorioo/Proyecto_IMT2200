{
    "principal": {
        "TEXT PRIMARY KEY"      : ["mlc"],
        "TEXT UNIQUE"           : ["link"],
        "BOOLEAN DEFAULT FALSE" : ["was_scraped"],
        "TEXT"      : ["inmueble", "modalidad", "region", "comuna", "titulo", "tiempo_publicado", "Barrio", "Dirección", "Descripción", "Superficie total unidad"],
        "TIMESTAMP" : ["timestamp", "timestamp_"],
        "BOOLEAN"   : ["get_highlighted_specs", "get_full_specs", "get_sector_prices", "get_location", "get_description", "get_full_specs_bin"],
        "REAL"      : ["Superficie total", "UF/m² esta propiedad declarado", "UF/m² zona declarado",  "lat", "lon"],
        "INTEGER"   : ["Dormitorios", "Baños", "UF", "gastos_comunes", "US$", "$"]
    },
    "full_specs": {
        "TEXT PRIMARY KEY" : ["mlc"],
        "TEXT"    : ["Superficie total unidad", "Superficie útil unidad", "Gastos comunes unidad", "Orientación"],
        "REAL"    : ["Superficie total", "Superficie útil"],
        "INTEGER" : ["Dormitorios", "Baños", "Estacionamientos", "Cantidad de pisos", "Gastos comunes", "Bodegas"]
    },

    "full_specs_bin": {
        "TEXT PRIMARY KEY" : ["mlc"]
    }
}